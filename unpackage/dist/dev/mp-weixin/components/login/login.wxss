.login-dialong {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  background-color: #fff;
  border-radius: 24rpx 24rpx 0 0;
  height: 50vh;
  padding: 50rpx 40rpx;
}
.login-dialong .close-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
